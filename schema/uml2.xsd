	<xs:element name="Message.sendEvent">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="EventOccurrence" />
			</xs:choice>
		</xs:complexType>		
	</xs:element>

	<xs:element name="Message.receiveEvent">
		<xs:complexType>
