			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
		</xs:complexType>		
	</xs:element>
		
	<xs:element name="MessageEnd.sendMessage">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Message"/>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
	
	<xs:element name="MessageEnd.receiveMessage">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Message"/>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
	
	<xs:element name="OpaqueExpression">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="body" />
			<xs:attribute ref="language" />
		</xs:complexType>		
	</xs:element>
		
	<xs:element name="Lifeline">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Lifeline.coveredBy"/>
				<xs:element ref="Lifeline.represents"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
		</xs:complexType>		
	</xs:element>
	
	<xs:element name="Lifeline.coveredBy">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="EventOccurrence"/>
				<xs:element ref="ExecutionOccurrence"/>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
	
	<xs:element name="Lifeline.represents">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Attribute"/>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
	
	<!-- Activity -->
	<xs:element name="Activity">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Namespace.ownedElement"/>
				<xs:element ref="Activity.edge"/>
				<xs:element ref="Activity.node"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
			<xs:attribute ref="isRoot" />
			<xs:attribute ref="isLeaf" />
			<xs:attribute ref="isAbstract" />
			<xs:attribute ref="isActive" />
			<xs:attribute ref="isReadOnly" />
			<xs:attribute ref="isSingleExecution" />
		</xs:complexType>	
	</xs:element>
	
	<xs:element name="Activity.edge">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityEdge"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="Activity.node">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="InitialNode"/>
				<xs:element ref="CallAction"/>
				<xs:element ref="AcceptEventAction"/>
				<xs:element ref="DecisionNode"/>
				<xs:element ref="MergeNode"/>
				<xs:element ref="ForkNode"/>
				<xs:element ref="JoinNode"/>
				<xs:element ref="FlowFinalNode"/>
				<xs:element ref="ActivityFinalNode"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityEdge">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityEdge.source"/>
				<xs:element ref="ActivityEdge.target"/>
				<xs:element ref="ActivityEdge.guard"/>
				<xs:element ref="ActivityEdge.weight"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>		
	</xs:element>
	
	<xs:element name="ActivityEdge.source">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="InitialNode"/>
				<xs:element ref="CallAction"/>
				<xs:element ref="AcceptEventAction"/>
				<xs:element ref="DecisionNode"/>
				<xs:element ref="MergeNode"/>
				<xs:element ref="ForkNode"/>
				<xs:element ref="JoinNode"/>
				<xs:element ref="OutputPin"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityEdge.target">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="CallAction"/>
				<xs:element ref="DecisionNode"/>
				<xs:element ref="MergeNode"/>
				<xs:element ref="ForkNode"/>
				<xs:element ref="JoinNode"/>
				<xs:element ref="FlowFinalNode"/>
				<xs:element ref="ActivityFinalNode"/>
				<xs:element ref="InputPin"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityEdge.guard">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="OpaqueExpression"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityEdge.weight">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="OpaqueExpression"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="InitialNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="DecisionNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="MergeNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ForkNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="JoinNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="JoinNode.joinSpec"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
			<xs:attribute ref="isCombinedDuplicate" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="FlowFinalNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityFinalNode">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="CallAction">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="Action.output"/>
				<xs:element ref="Action.input"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="AcceptEventAction">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityNode.outgoingEdge">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityEdge"/>				
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ActivityNode.incomingEdge">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ActivityEdge"/>				
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="JoinNode.joinSpec">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="OpaqueExpression"/>				
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="Action.output">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="OutputPin"/>
			</xs:choice>
		</xs:complexType>	
	</xs:element>
	
	<xs:element name="Action.input">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="InputPin"/>
			</xs:choice>
		</xs:complexType>	
	</xs:element>
	
	<xs:element name="OutputPin">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ObjectNode.upperBound"/>
				<xs:element ref="ActivityNode.outgoingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
			<xs:attribute ref="isOrdered" />
			<xs:attribute ref="isControl" />
			<xs:attribute ref="isControlType" />
			<xs:attribute ref="ordering" />
			<xs:attribute ref="isUnique" />
			<xs:attribute ref="upper" />
			<xs:attribute ref="lower" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="InputPin">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ObjectNode.upperBound"/>
				<xs:element ref="ActivityNode.incomingEdge"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
			</xs:choice>
			<xs:attribute ref="xmi.id" />
			<xs:attribute ref="xmi.idref" />
			<xs:attribute ref="name" />
			<xs:attribute ref="visibility" />
			<xs:attribute ref="isSpecification" />
			<xs:attribute ref="isOrdered" />
			<xs:attribute ref="isControl" />
			<xs:attribute ref="isControlType" />
			<xs:attribute ref="ordering" />
			<xs:attribute ref="isUnique" />
			<xs:attribute ref="upper" />
			<xs:attribute ref="lower" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ObjectNode.upperBound">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="OpaqueExpression"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>	


	<!--
		Attribute definitions
	-->
	<xs:attribute name="xmi.id" type="xs:string" />
	<xs:attribute name="xmi.idref" type="xs:string" />
	<xs:attribute name="xmi.name" type="xs:string" />
	<xs:attribute name="xmi.version" type="xs:string" />
	<xs:attribute name="name" type="xs:string" />
	<xs:attribute name="location" type="xs:string" />
	<xs:attribute name="visibility" type="visibilityType" />
	<xs:attribute name="ordering" type="orderingType" />
	<xs:attribute name="aggregation" type="aggregationType" />
	<xs:attribute name="ownerScope" type="xs:string" />
	<xs:attribute name="targetScope" type="targetScopeType" />
	<xs:attribute name="changeability" type="changeabilityType" />
	<xs:attribute name="isRoot" type="xs:boolean" />
	<xs:attribute name="isSpecification" type="xs:boolean" />
	<xs:attribute name="isConcurrent" type="xs:boolean" />
	<xs:attribute name="isLeaf" type="xs:boolean" />
	<xs:attribute name="isAbstract" type="xs:boolean" />
	<xs:attribute name="isActive" type="xs:boolean" />
	<xs:attribute name="isNavigable" type="xs:boolean" />
	<xs:attribute name="isQuery" type="xs:boolean" />
	<xs:attribute name="isInstanciable" type="xs:boolean" />
	<xs:attribute name="lower" type="xs:string" />
	<xs:attribute name="upper" type="xs:string" />
	<xs:attribute name="tagType" type="xs:string" />
	<xs:attribute name="language" type="xs:string" />
	<xs:attribute name="body" type="xs:string" />
	<xs:attribute name="concurrency" type="concurrencyType" />
	<xs:attribute name="kind" type="kindType" />
	<!--
		UML2 Attribute definitions
	-->
	<xs:attribute name="messageSort" type="messageSortType" />
	<xs:attribute name="isReadOnly" type="xs:boolean"/>
	<xs:attribute name="isSingleExecution" type="xs:boolean"/>
	<xs:attribute name="isCombinedDuplicate" type="xs:boolean"/>
	<xs:attribute name="isControlType" type="xs:boolean"/>
	<xs:attribute name="isControl" type="xs:boolean"/>
	<xs:attribute name="isOrdered" type="xs:boolean"/>
	<xs:attribute name="isUnique" type="xs:boolean"/>
	

	<!--
		Type definitions
	-->
	<xs:simpleType name="visibilityType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="public" />
			<xs:enumeration value="protected" />
			<xs:enumeration value="private" />
			<xs:enumeration value="package" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="changeabilityType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="changeable" />
			<xs:enumeration value="frozen" />
			<xs:enumeration value="addOnly" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="orderingType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="unordered" />
			<xs:enumeration value="ordered" />
			<xs:enumeration value="FIFO" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="aggregationType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="none" />
			<xs:enumeration value="aggregate" />
			<xs:enumeration value="composite" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="targetScopeType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="instance" />
			<xs:enumeration value="classifier" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="concurrencyType">
		<xs:restriction base='xs:NMTOKEN'>
			<xs:enumeration value='sequential'/>
			<xs:enumeration value='guarded'/>
			<xs:enumeration value='concurrent'/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="kindType">
		<xs:restriction base='xs:NMTOKEN'>
			<xs:enumeration value='in'/>
			<xs:enumeration value='inout'/>
			<xs:enumeration value='out'/>
			<xs:enumeration value='return'/>
			<xs:enumeration value='external'/>
			<xs:enumeration value='initial'/>
		</xs:restriction>
	</xs:simpleType>
	
	<!--
		UML2 Type definitions
	-->
	<xs:simpleType name="messageSortType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="synchCall" />
			<xs:enumeration value="asynchCall" />
		</xs:restriction>
	</xs:simpleType>

	<!--
		XMI Diagram (not supported yet)
	-->
	<xs:element name="Diagram">
		<xs:complexType>
			<xs:sequence>
				<xs:any namespace='http://www.w3.org/namespace/'/>
			</xs:sequence>
			<xs:attribute name='property' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='reference' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='container' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='semanticModel' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='link' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='contained' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='anchorage' type='xs:IDREFS' use='optional'/>
			<xs:attribute name='diagramLink' type='xs:IDREFS' use='optional'/>
