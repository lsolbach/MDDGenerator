	<xs:element name="Message.signature">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Operation" />
			</xs:choice>
		</xs:complexType>		
	</xs:element>

	<xs:element name="Message.argument">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="OpaqueExpression" />
			</xs:choice>
		</xs:complexType>		
	</xs:element>

	<xs:element name="MessageEnd">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="InteractionFragment.covered"/>
				<xs:element ref="ModelElement.stereotype" />
				<xs:element ref="ModelElement.taggedValue" />
