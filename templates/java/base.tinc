<?
list repositoryElementList() {
	list elementList
	foreach package <- Packages {
		foreach element
				| !isAbstract(element) && isInstanceOf(element, 'Classifier') && !element:StereotypeMap 
				<- package:Classes {
			elementList.add(element)
		}	
	}
	elementList
}

list factoryElementList() {
	list elementList
	foreach package <- Packages {
		foreach element
				| !isAbstract(element) && !element:StereotypeMap
				<- package:Classes {
			elementList.add(element)
		}	
	}
	elementList
}
?>
